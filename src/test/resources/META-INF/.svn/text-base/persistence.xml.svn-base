<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
	version="2.0" xmlns="http://java.sun.com/xml/ns/persistence">
	<persistence-unit name="Knoledgehub">
		<class>org.ufsc.knoledgehub.model.entity.Discipline</class>
		<class>org.ufsc.knoledgehub.model.entity.User</class>
		<class>org.ufsc.knoledgehub.model.entity.ProgrammingLanguage</class>
		<class>org.ufsc.knoledgehub.model.entity.Project</class>
		<properties>
			<property name="eclipselink.logging.level" value="WARNING"/>
			<property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver" />
			<property name="javax.persistence.jdbc.url"	value="jdbc:mysql://localhost:3306/knoledgehub" />
			<property name="javax.persistence.jdbc.user" value="root" />
			<property name="javax.persistence.jdbc.password" value="" />
		</properties>
	</persistence-unit>
</persistence>